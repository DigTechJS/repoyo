<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="surveysVM" type="com.repoyo.app.modules.surveys.data.viewmodel.SurveysVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSurveys"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumniphonexorne"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imageIPhoneXorne"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_iphonexorne"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iphonexorne"/>
            <LinearLayout android:id="@+id/linearRowvector"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_bluegray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_bluegray_900"/>
                <LinearLayout android:id="@+id/linearRowmedia"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_141pxh"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMedia"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.76"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{surveysVM.surveysModel.txtMedia}"
                        style="@style/txtGilroysemibold24" tools:text="@string/lbl_media"/>
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_100pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                    <ImageView android:id="@+id/imageMorevertical"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_29pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_2pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_morevertical"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerSurveys"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="12" tools:listitem="@layout/row_surveys"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumn"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900_b2">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnrateyourexper"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_373pxv"
                android:layout_marginEnd="@dimen/_65pxh"
                android:layout_marginStart="@dimen/_65pxh"
                android:layout_marginTop="@dimen/_372pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtRateYourExper"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{surveysVM.surveysModel.txtRateYourExper}"
                    style="@style/txtGilroysemibold18_1" tools:text="@string/msg_rate_your_exper"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtAreyousatisfi"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{surveysVM.surveysModel.txtAreyousatisfi}"
                    style="@style/txtGilroymedium14_1" tools:text="@string/msg_are_you_satisfi"/>
                <ImageView android:id="@+id/imageFrame9866"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_31pxv"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="@dimen/_137pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_frame9866"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_frame9866"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
