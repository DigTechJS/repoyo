<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notes1RowModel" type="com.repoyo.app.modules.notes.data.model.Notes1RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowaprcounter"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_weight="0.5" android:layout_width="0dp"
            android:orientation="vertical" style="@style/groupStylecornerRadius_4">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnaprcounter"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAprCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{notes1RowModel.txtAprCounter}"
                    style="@style/txtGilroyregular14_1" tools:text="@string/lbl_20_apr"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtExplorationide"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{notes1RowModel.txtExplorationide}"
                    style="@style/txtGilroysemibold16" tools:text="@string/msg_exploration_ide"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearTags1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_6">
                    <LinearLayout android:id="@+id/linearRowframeeight"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_6">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFrameEight"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.32"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_4pxv"
                            android:paddingEnd="@dimen/_5pxh"
                            android:paddingStart="@dimen/_5pxh"
                            android:paddingTop="@dimen/_4pxv"
                            android:singleLine="true"
                            android:text="@{notes1RowModel.txtFrameEight}"
                            style="@style/txtSolidRounded" tools:text="@string/lbl_design"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFrameNine"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_weight="0.51"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_4pxv"
                            android:paddingEnd="@dimen/_5pxh"
                            android:paddingStart="@dimen/_5pxh"
                            android:paddingTop="@dimen/_4pxv"
                            android:singleLine="true"
                            android:text="@{notes1RowModel.txtFrameNine}"
                            style="@style/txtSolidRounded" tools:text="@string/lbl_productivity"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFrameTen"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_weight="0.17"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_4pxv"
                            android:paddingEnd="@dimen/_5pxh"
                            android:paddingStart="@dimen/_5pxh"
                            android:paddingTop="@dimen/_4pxv"
                            android:singleLine="true"
                            android:text="@{notes1RowModel.txtFrameTen}"
                            style="@style/txtSolidRounded" tools:text="@string/lbl_12"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@{notes1RowModel.txtDescription}"
                    style="@style/txtGilroyregular14_3" tools:text="@string/msg_ticket_app_trav"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnaprcounterOne"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_weight="0.5" android:layout_width="0dp"
            android:orientation="vertical" style="@style/groupStylebluegray_1">
            <TextView android:ellipsize="end"
                android:id="@+id/txtAprCounterOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{notes1RowModel.txtAprCounterOne}"
                style="@style/txtGilroyregular14_1" tools:text="@string/lbl_19_apr"/>
            <TextView android:id="@+id/txtDatabaseSystem"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_10pxh"
                android:text="@{notes1RowModel.txtDatabaseSystem}"
                style="@style/txtGilroysemibold16" tools:text="@string/msg_database_syste"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTags2"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_6">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameEightOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.32"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_5pxh"
                    android:paddingStart="@dimen/_5pxh"
                    android:paddingTop="@dimen/_4pxv"
                    android:singleLine="true"
                    android:text="@{notes1RowModel.txtFrameEightOne}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_design"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameNineOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.51"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_5pxh"
                    android:paddingStart="@dimen/_5pxh"
                    android:paddingTop="@dimen/_4pxv"
                    android:singleLine="true"
                    android:text="@{notes1RowModel.txtFrameNineOne}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_productivity"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameTenOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.17"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingEnd="@dimen/_5pxh"
                    android:paddingStart="@dimen/_5pxh"
                    android:paddingTop="@dimen/_4pxv"
                    android:singleLine="true"
                    android:text="@{notes1RowModel.txtFrameTenOne}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_12"/>
            </LinearLayout>
            <TextView android:id="@+id/txtTodaysShoppng"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{notes1RowModel.txtTodaysShoppng}"
                style="@style/txtGilroyregular14_3" tools:text="@string/msg_today_s_shoppng"/>
        </LinearLayout>
    </LinearLayout>
</layout>
