<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="saveAsPdfRowModel" type="com.repoyo.app.modules.saveaspdf.data.model.SaveAsPdfRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRow1200pxpdffile"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/image1200pxPDFfile"
            android:layout_height="@dimen/_29pxh"
            android:layout_marginBottom="@dimen/_22pxv"
            android:layout_width="@dimen/_24pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_1200pxpdffile"
            tools:ignore="ContentDescription" tools:src="@drawable/img_1200pxpdffile"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnweburl"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_3pxv"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_weight="0.84" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtWeburl"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{saveAsPdfRowModel.txtWeburl}"
                style="@style/txtGilroymedium16" tools:text="@string/lbl_file_01_pdf"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFilesize"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{saveAsPdfRowModel.txtFilesize}"
                style="@style/txtGilroyregular14_1" tools:text="@string/msg_03_04_2021_7"/>
        </LinearLayout>
    </LinearLayout>
</layout>
