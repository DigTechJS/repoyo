<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchRowModel" type="com.repoyo.app.modules.search.data.model.SearchRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowvectorOne"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowvector"
            android:layout_height="wrap_content"
            android:layout_weight="0.91" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVectorOne1"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_bluegray_400_12x12"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_bluegray_400_12x12"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLoremipsum"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{searchRowModel.txtLoremipsum}"
                style="@style/txtGilroymedium16" tools:text="@string/lbl_lorem_ipsum"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageVectorOne2"
            android:layout_height="@dimen/_12pxh"
            android:layout_marginBottom="@dimen/_2pxv"
            android:layout_marginStart="@dimen/_245pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_12pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_12x12"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_12x12"/>
    </LinearLayout>
</layout>
